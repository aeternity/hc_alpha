<h4 class="mt-4">Validators</h4>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Validator</th>
      <th scope="col">Status</th>
      <th scope="col">Stake</th>
    </tr>
  </thead>
  <tbody>
    <%= for v <- @validators do %>
      <tr>
        <td><%= v.address %></td>
        <td><%= v.status %></td>
        <td><%= v.stake %> AE</td>
      </tr>
    <% end %>
  </tbody>
</table>
